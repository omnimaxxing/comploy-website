	"description": "PayloadPlugins website, an open-source directory for all things PayloadCMS",
	"license": "MIT",
	"type": "module",
	"packageManager": "pnpm@10.3.0",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"build": "bash ./vercel.sh",
		"prod": "cross-env NODE_OPTIONS=--no-deprecation  payload migrate && next build",
		"dev": "cross-env NODE_OPTIONS=--no-deprecation  next dev ",
		"dev:prod": "cross-env NODE_OPTIONS=--no-deprecation PAYLOAD_SKIP_MIGRATIONS=true rm -rf .next && next build && next start",
		"generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
		"generate:types": "cross-env NODE_OPTIONS='--no-deprecation --no-warnings' payload generate:types",
		"ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
		"lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
		"lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
		"prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
		"prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
		"payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
		"reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm -rf .next && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
		"start": "cross-env NODE_OPTIONS=--no-deprecation next start",
		"format": "prettier --write ./src",
		"postbuild": "next-sitemap --config next-sitemap.config.cjs",
		"tunnel": "lt --port 3000 --subdomain omnipixel-dev",
		"setup": "node scripts/simple-setup.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"list": "node scripts/dev.js list",
		"generate": "node scripts/dev.js generate",
		"parse-prd": "node scripts/dev.js parse-prd"
	},
